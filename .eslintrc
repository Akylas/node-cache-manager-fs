{
  "rules": {
    "block-scoped-var": 2,
    "complexity": [ 2, 10 ],
    "curly": 2,
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 1,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-magic-numbers": 0,    
    "no-native-reassign": 2,    
    "no-proto": 1,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 1,
    "no-useless-concat": 1,
    "no-warning-comments": 1,
    "no-mixed-spaces-and-tabs": 0,
    "yoda": 1,

    "camelcase": 1,
    "eol-last": 1,
    "func-names": 1,
    "semi": [ 2, "always" ],    
    "quotes": [ 1, "single" ],    
    "max-nested-callbacks": [ 1, 5 ],
    "new-cap": 1,
    "new-parens": 1,
    "no-lonely-if": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": [ 0, { "skipBlankLines": true } ],
    "no-unneeded-ternary": 1,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-keywords": 1,
    "space-in-parens": 1,
    "space-infix-ops": 2,
    "space-unary-ops": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "angular": true
  },
  "extends": "eslint:recommended"
}
